#!/bin/bash
if [[ $# -gt 0 || "$*" = "help" || "$*" = "-h" || "$*" = "--help" ]]; then
cat << EOF
Syntax: web_stop

Stops looping Node-RED.

EOF
exit 1
fi

[ "$IOTEMPOWER_ACTIVE" = "yes" ] || { echo "IoTempower not active, aborting." 1>&2;exit 1; }

pkill -f /bin/web_starter
pkill -f caddy
cloudcmd_stop
nodered_stop

